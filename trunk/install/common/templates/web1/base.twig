<html>
	<head>
		<title>{{ PageTitle }}</title>

		<script>
			var path_root = '{{ system.paths.controller }}';
			var path_resources = '{{ system.paths.resources }}';
			var path_js = '{{ system.paths.jscript }}';
		</script>

		<link rel="stylesheet" type="text/css" href="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.9.2/themes/redmond/jquery-ui.css" />
		<script type="text/javascript" src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.8.2.min.js"></script>
		<script type="text/javascript" src="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.9.2/jquery-ui.min.js"></script>
		<script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/jquery.blockUI/2.61.0-2013.06.06/jquery.blockUI.min.js"></script>

		<script type="text/javascript" src="{{ system.paths.rdparty }}RGraph/libraries/RGraph.common.core.js"></script>
		<script type="text/javascript" src="{{ system.paths.rdparty }}/RGraph/libraries/RGraph.line.js"></script>
		<script type="text/javascript" src="{{ system.paths.rdparty }}/RGraph/libraries/RGraph.bar.js"></script>
		<script type="text/javascript" src="{{ system.paths.rdparty }}/RGraph/libraries/RGraph.scatter.js"></script>
		<script type="text/javascript" src="{{ system.paths.rdparty }}/RGraph/libraries/RGraph.common.dynamic.js"></script>

		{% include "includes.twig" with { "file_list" : web1_js_files, "path": "web1", "type": "js", "defaults": [ 'script.js' ] } %}
		{% include "includes.twig" with { "file_list" : web1_css_files, "path": "web1", "type": "css", "defaults": [ 'style.css' ] } %}

		<script>
			path_include = "{{ system.paths.controller }}";
		</script>
	</head>
	<body>
		{% block Body %}
		{% endblock %}
	</body>
</html>
